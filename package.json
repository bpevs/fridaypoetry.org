{
  "name": "fridaypoetry.org",
  "version": "1.0.0",
  "description": "For writing poetry. On Friday.",
  "author": "Benjamin Pevsner",
  "license": "MIT",
  "main": "src/index.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivebencrazy/fridaypoetry.org.git"
  },
  "keywords": [
    "poetry"
  ],
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "deploy": "npm test && npm run build && cp ./build/index.html ./build/404.html",
    "dev": "preact watch",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "tslint --project ./tslint.json",
    "test:unit": "echo 'No unit testing yet'"
  },
  "dependencies": {
    "@zuck/core": "git+https://github.com/Blanket-Warriors/Zuck.git#latest-build",
    "@zuck/firebase": "git+https://github.com/Blanket-Warriors/Zuck-Firebase.git#latest-build",
    "lodash-es": "^4.17.4",
    "preact": "^8.2.5",
    "preact-async-route": "^2.0.0",
    "preact-compat": "^3.17.0",
    "preact-redux": "^2.0.2",
    "preact-router": "^2.5.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "xss-filters": "^1.2.7"
  },
  "devDependencies": {
    "@types/jest": "^21.1.0",
    "@types/lodash": "^4.14.74",
    "@types/redux": "^3.6.0",
    "@types/xss-filters": "0.0.27",
    "if-env": "^1.0.0",
    "jest": "^21.1.0",
    "postcss": "^6.0.11",
    "preact-cli": "^1.4.1",
    "preact-cli-plugin-typescript": "^0.2.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-jest": "^21.0.1",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  }
}
