{
  "collectCoverageFrom": [
    "src/components/**/*.{ts,tsx}",
    "src/routes/**/*.{ts,tsx}",
    "src/store/**/*.{ts,tsx}"
  ],
  "moduleFileExtensions": [ "ts", "tsx", "js", "jsx" ],
  "transform": {
    "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "transformIgnorePatterns": [
    "<rootDir>/node_modules/(?!@zuck)"
  ],
  "testMatch": ["**/*.test.(ts|tsx|js)"]
}
